osta/sublime-php-getters-setters/master/packages.json": [{"labels": ["auto-complete", "php", "snippets", "code generation", "text manipulation", "formatting"], "buy": null, "author": "Nuno Franco da Costa", "description": "generare getters and setters for php classes", "releases": [{"version": "2.0.8", "platforms": ["*"], "url": "https://codeload.github.com/francodacosta/sublime-php-getters-setters/zip/2.0.8", "date": "2013-10-22 17:17:55", "sublime_text": ">=3000"}, {"version": "1.0.10", "platforms": ["*"], "url": "https://codeload.github.com/francodacosta/sublime-php-getters-setters/zip/1.0.10", "date": "2013-09-06 00:00:00", "sublime_text": "<3000"}], "donate": "https://www.gittip.com/on/github/francodacosta/", "readme": "https://raw.github.com/francodacosta/sublime-php-getters-setters/master/Readme.md", "name": "PHP Getters and Setters", "homepage": "https://github.com/francodacosta/sublime-php-getters-setters", "previous_names": [], "issues": "https://github.com/francodacosta/sublime-php-getters-setters/issues"}], "https://raw.github.com/apophys/sublime-packages/master/packages.json": [{"labels": [], "buy": null, "author": "apophys", "description": "Simple manual page \"viewer\".", "releases": [{"version": "2013.12.27.23.52.18", "platforms": ["*"], "url": "https://codeload.github.com/apophys/SublimeManpage/zip/Sublime-Text-3", "date": "2013-12-27 23:52:18", "sublime_text": ">=3000"}, {"version": "1.0.0", "platforms": ["*"], "url": "https://codeload.github.com/apophys/SublimeManpage/zip/v1.0.0", "date": "2013-08-20 09:37:16", "sublime_text": "<3000"}], "donate": "https://www.gittip.com/on/github/apophys/", "readme": "https://raw.github.com/apophys/SublimeManpage/master/README.md", "name": "SublimeManpage", "homepage": "https://github.com/apophys/SublimeManpage", "previous_names": [], "issues": "https://github.com/apophys/SublimeManpage/issues"}], "https://raw.github.com/afterdesign/MacTerminal/master/packages.json": [{"labels": ["terminal"], "buy": null, "author